{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Jwt": {
    "Key": "veryVerySecretKey",
    "Issuer": "http://localhost:5000/",
    "Audience": "http://localhost:5000/",
    "ExpiryMinutes" : 20
  },
  "Cookie":{
    "Name" : "jwt_cookie",
    "ExpiryMinutes" : 20
  },
  "AppDataSecurity":{
    "symmetricKey" : "symmetricKey",
    "rijndaelManagedPaddingMode" : "PKCS7",
    "isLegacyJWT" : false
  },
  "AppSettings":{
    "appRootPath" : "/",
    "loginUrl" : "/public/index",
    "homePageUrl" : "/protected/index"
  },
  "APIProxySettings":{
    "proxyPath" : "/api/proxy",
    "apiUrlQueryStringKey" : "url",
    "bufferLengthInKB" : 4,
    "clientSettings": {
      "jsonPlaceHolderBasePath": "http://jsonplaceholder.typicode.com",
      "pwaapiFxPlaceHolderBasePath": "https://pwawebapifx.azurewebsites.net/api"
    }
  }
}
